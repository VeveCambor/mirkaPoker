{
    "version": 2,
    "builds": [
      { "src": "api/index.ts", "use": "@vercel/node" },
      { "src": "client/package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.ts" },
      { "src": "/(.*)", "dest": "/client/dist/$1" }
    ]
  }
  